{
  "name": "cypress-ci",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:ci": "ng test --watch=false --code-coverage=true --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "lint:ci": "ng lint",
    "e2e": "ng e2e",
    "ci": "run-s lint:ci build:prod ci:cy-run",
    "ci:cy-run": "start-server-and-test ci:start-server http://localhost:4200 cy:run",
    "ci:start-server": "angular-http-server --path ./dist/CypressCi -p 4200 --silent",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:verify": "cypress verify",
    "ci:teamcity": "run-s lint:cs build:prod ci:tc-start-server",
    "ci:teamcity-start-server": "start-server-and-test ci:start-server http://localhost:4200 ci:teamcity-cy-run",
    "ci:teamcity-cy-run": "cypress run --reporter mocha-teamcity-reporter"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "cypress": "^3.1.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "start-server-and-test": "^1.7.11",
    "ts-loader": "^5.3.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "mocha-teamcity-reporter": "^3.0.0",
    "angular-http-server": "^1.9.0"
  }
}
